<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:orientation="vertical"    android:background="@drawable/conner_and_border">    <com.beardedhen.androidbootstrap.FontAwesomeText        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:textSize="@dimen/sp_20"        android:textColor="#CFB53B"        app:fa_icon="fa-exclamation"        android:layout_alignParentLeft="true"        android:layout_marginLeft="@dimen/dp_10"        android:layout_marginTop="@dimen/dp_10"/>    <TextView        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:text="气虚质饮食注意"        android:id="@+id/title"        android:layout_centerHorizontal="true"        android:layout_marginTop="@dimen/dp_10"        android:textColor="#CFB53B"        android:textSize="@dimen/sp_16"/>    <View        android:id="@+id/line1"        android:layout_width="match_parent"        android:layout_height="@dimen/dp_1"        android:background="#CCCCCC"        android:layout_below="@id/title"        android:layout_marginTop="@dimen/dp_10"        android:layout_marginBottom="@dimen/dp_10" />    <com.beardedhen.androidbootstrap.FontAwesomeText        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:textSize="@dimen/sp_16"        android:textColor="@color/green_dark"        app:fa_icon="fa-hand-o-right"        android:layout_below="@id/line1"        android:id="@+id/hands"        android:layout_alignLeft="@id/content"/>    <TextView        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:textColor="@color/grey_font2"        android:text="Tips:"        android:layout_toRightOf="@id/hands"        android:layout_alignTop="@id/hands"        android:layout_marginLeft="@dimen/dp_10"/>    <TextView        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:id="@+id/content"        android:text="您是气虚体质，尽量少吃萝卜"        android:layout_marginLeft="@dimen/dp_10"        android:layout_marginRight="@dimen/dp_10"        android:lineSpacingMultiplier="1.5"        android:layout_below="@id/hands"        android:textColor="@color/grey_8e"        android:layout_marginTop="@dimen/dp_10"/>    <TextView        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_below="@id/content"        android:layout_alignParentRight="true"        android:layout_marginRight="@dimen/dp_10"        android:text="查看更多"        android:layout_marginTop="@dimen/dp_5"        android:layout_marginBottom="@dimen/dp_5"/></RelativeLayout>