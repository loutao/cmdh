<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="1.0dip" />
            <gradient android:startColor="@color/feed_color_background" android:endColor="@color/feed_color_background" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="1.0dip" />
                <gradient android:startColor="@color/codoon_green" android:endColor="@color/codoon_green" />
            </shape>
        </clip>
    </item>
</layer-list>